{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": [
    "**/.env.*"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
        // "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "build"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [],
      "cache": false
    }
  }
}